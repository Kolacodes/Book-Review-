                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/">ReadRight</a>
            </div>
            <div id="navbarCollapse" class="collapse navbar-collapse">
                <ul class="nav navbar-nav ">
                    <li>    <a href="/books"><span class="glyphicon glyphicon-home"></span> Home</a>    </li>
                    <li>    <a href=""><span class="glyphicon glyphicon-info-sign"></span> About</a>    </li>
                    <li>    <a href=""><i class="material-icons md-18">lightbulb_outline</i> Gists</a>    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <% if(!currentUser){ %>
                        <li><a href="/login"><span class="glyphicon glyphicon-log-in"  aria-hidden="true" ></span> Login</a></li>
                        <li><a href="/register"><i class="material-icons md-18">person_add</i> Sign Up</a></li>
                    <% } else { %>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="glyphicon glyphicon-user"></span>
                                <strong><%=currentUser.username%></strong>
                                <span class="glyphicon glyphicon-chevron-down"></span>
                            </a>
                            <ul role="menu" class="dropdown-menu">
                                <li>
                                    <div class="navbar-login">
                                        <div class="row">
                                            <div class="col-lg-4">
